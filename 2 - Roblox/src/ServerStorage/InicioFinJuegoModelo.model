-- src/ServerStorage/InicioFinJuegoModelo.model
local model = Instance.new("Model")
model.Name = "InicioFinJuegoModelo"

local base = Instance.new("Part")
base.Name = "Base"
base.Size = Vector3.new(10, 1, 10) -- Tamaño básico de una plataforma
base.Position = Vector3.new(0, 0, 0)
base.Anchored = true
base.BrickColor = BrickColor.new("Bright green")
base.Parent = model

model.PrimaryPart = base -- Asigna la base como PrimaryPart

return model